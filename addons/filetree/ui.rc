<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kpartgui>
<gui name="katefiletreeplugin" library="katefiletreeplugin" version="7">
  <!-- merge in view -->
  <MenuBar>
    <Menu name="view">
      <text>&amp;View</text>
      <Action name="filetree_prev_document" group="switch_document"/>
      <Action name="filetree_next_document" group="switch_document"/>
      <Action name="filetree_show_active_document" group="switch_document"/>
      <Action name="filetree_save" group="save_document"/>
      <Action name="filetree_save_as" group="save_document"/>
    </Menu>
  </MenuBar>
</gui>
<!-- kate: space-indent on; indent-width 4; replace-tabs on; -->
